test =>

  s := lock_free.stack i32

  i := concur.atomic 0
  count(n i32) =>
    c := i.read
    i.compare_and_set c c+n

  (1..4)
    .map_sequence (_)->
      concur.thread.spawn ()->
        for i in 1..100 do
          s.push i
          p := s.pop
          while !(count p.get)
    .as_array
    .for_each (t)->
      t.join

  say i.read


